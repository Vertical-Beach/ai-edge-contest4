name: "unet"
layer {
  name: "data"
  type: "ImageSegData"
  top: "data"
  top: "label"
  transform_param {
    crop_size: 512
    mean_value: 73
    mean_value: 82
    mean_value: 72
    scale: 0.022
  }
  image_data_param {
    source: "/workspace/Segment/workspace/data/cityscapes/img_seg.txt"
    batch_size: 4
    shuffle: true
    mirror: true
  }
 include {
   phase: TRAIN
  }
}


layer {
  name: "conv_d0a-b"
  type: "Convolution"
  bottom: "data"
  top: "d0b"
  param {
    lr_mult: 1
    decay_mult: 1
  }
  param {
    lr_mult: 2
    decay_mult: 0
  }
  convolution_param {
    num_output: 32
    pad: 1
    kernel_size: 3
    stride: 1
    weight_filler {
      type: "msra"
    }
  }
}



layer {
  name: "relu_d0b"
  type: "ReLU"
  bottom: "d0b"
  top: "relu_d0b"
}
